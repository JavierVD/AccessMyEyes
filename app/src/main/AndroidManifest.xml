        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AccessMyEyes"
        tools:targetApi="31">
        <activity
            android:name=".master_server_view"
            android:exported="false"
            android:label="@string/title_activity_master_server_view"
            android:theme="@style/Theme.AccessMyEyes.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".mainServerView"
            android:exported="false"
            android:label="@string/title_activity_main_server_view"
            android:theme="@style/Theme.AccessMyEyes.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
